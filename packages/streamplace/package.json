{
  "name": "streamplace",
  "version": "0.2.0",
  "description": "An open-source compositor and CMS for live video",
  "main": "dist/streamplace.js",
  "scripts": {
    "start": "babel --copy-files -d dist -s -w src",
    "prepare": "babel -s -d dist src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "4"
          }
        }
      ]
    ]
  },
  "bin": {
    "stream": "bin/streamplace.js",
    "streamplace": "bin/streamplace.js"
  },
  "keywords": [
    "streamplace"
  ],
  "author": "Streamplace",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^3.1.0",
    "regenerator-runtime": "^0.11.1",
    "sp-api-server": "^0.2.0",
    "sp-client": "^0.2.0",
    "sp-compositor": "^0.2.0",
    "sp-coturn": "^0.2.0",
    "sp-default": "^0.2.0",
    "sp-file-server": "^0.2.0",
    "sp-frontend": "^0.2.0",
    "sp-ingress": "^0.2.0",
    "sp-kube-lego": "^0.2.0",
    "sp-plugin-core": "^0.2.0",
    "sp-redirects": "^0.2.0",
    "sp-registry": "^0.2.0",
    "sp-rethinkdb": "^0.2.0",
    "sp-rtmp-server": "^0.2.0",
    "sp-schema": "^0.2.0",
    "sp-scuttlebot": "^0.2.0",
    "sp-uploader": "^0.2.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-streamplace": "0.0.3"
  }
}
