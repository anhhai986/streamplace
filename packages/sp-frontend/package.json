{
  "name": "sp-frontend",
  "version": "0.1.9",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "electron": "^1.8.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "^1.1.1",
    "sp-static": "^0.1.9"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "main": "dist/sp-app-frontend.js",
  "dependencies": {
    "color-hash": "^1.0.3",
    "cookie": "^0.3.1",
    "font-awesome": "^4.7.0",
    "left-pad": "^1.1.3",
    "normalize.css": "^5.0.0",
    "prop-types": "^15.6.0",
    "qs": "^6.3.1",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^2.0.0",
    "react-burger-menu": "^2.1.11",
    "react-dom": "^16.2.0",
    "react-native-web": "^0.5.3",
    "react-router": "^4.0.0-beta.8",
    "react-router-dom": "^4.0.0-beta.8",
    "react-youtube": "^7.5.0",
    "sp-client": "^0.1.9",
    "sp-components": "^0.1.9",
    "sp-styles": "^0.1.9",
    "styled-components": "^1.4.4",
    "three": "^0.84.0",
    "url": "^0.11.0"
  },
  "scripts": {
    "dev": "bash -c 'PORT=80 react-scripts start | cat'",
    "start-cra": "PORT=3939 react-scripts start",
    "start-babel": "NODE_ENV=development babel -w -s -d dist src",
    "prepare-cra": "react-scripts build && rm -rf dist && mv build dist",
    "prepare-babel": "NODE_ENV=production babel -s -d dist src",
    "prepare": "run-s prepare-cra prepare-babel",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "run-p start-cra start-babel"
  }
}
