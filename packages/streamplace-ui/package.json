{
  "name": "streamplace-ui",
  "version": "0.2.0",
  "description": "Isomorphic front-end for Streamplace",
  "main": "dist/streamplace-ui.js",
  "scripts": {
    "start": "NODE_ENV=development babel -w --copy-files -s -d dist src",
    "prepare": "NODE_ENV=production babel --copy-files -s -d dist src"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "author": "Streamplace",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app": "^3.1.1"
  },
  "dependencies": {
    "auth0-js": "^9.4.1",
    "jwt-decode": "^2.2.0",
    "react-native-auth0": "^1.2.2",
    "react-native-vector-icons": "^4.6.0",
    "sp-builder": "^0.2.0",
    "sp-client": "^0.2.0",
    "styled-components": "^3.2.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
